# Rsync exclude patterns for TeleClaude
# Prevents overwriting local configuration when syncing between computers

# Local configuration files (NEVER sync these)
config.yml
.env
.env.*

# Database files (unique per computer)
*.db
*.db-shm
*.db-wal

# Git directory
.git/

# Python virtual environment
.venv/
__pycache__/
*.pyc
*.pyo
*.pyd
.Python

# Build artifacts
*.egg-info/
dist/
build/

# IDE files
.vscode/
.idea/

# OS files
.DS_Store
Thumbs.db

# Log files
*.log

# Temporary files
*.tmp
*.swp
*.swo
*~

# Upload directory (could contain sensitive user files)
uploads/

# Test coverage
.coverage
htmlcov/
.pytest_cache/

# Local development
.history/
trees/

# Session data
session-env/
shell-snapshots/

# MCP socket
*.sock
